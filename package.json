{
  "name": "@wattanx/nuxt-vuex",
  "version": "0.0.2",
  "description": "vuex module for Nuxt 3.",
  "repository": "https://github.com/wattanx/nuxt-vuex-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release --major --no-commit --no-tag",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "3.6.5",
    "hash-sum": "^2.0.0",
    "knitwork": "^1.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "3.6.5",
    "@nuxt/test-utils": "3.6.5",
    "@types/hash-sum": "^1.0.0",
    "@types/node": "^18.17.1",
    "changelogen": "^0.5.4",
    "eslint": "^8.46.0",
    "nuxt": "3.6.5",
    "vitest": "^0.33.0",
    "vuex": "^4.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
